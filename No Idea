# Enter your code here. Read input from STDIN. Print output to STDOUT
nm = input().split(" ")

n = int(nm[0])
m = int(nm[1])


arr = input().split()

a = set(input().split())
b = set( input().split())

ans = 0

for i in arr:
    if i in a:
        ans += 1
    if i in b:
        ans -= 1
print(ans)
